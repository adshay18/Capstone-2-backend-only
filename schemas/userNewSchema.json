{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://example.com/example.json",
	"type": "object",
	"properties": {
		"username": {
			"type": "string",
			"minLength": 1,
			"maxLength": 30,
			"default": ""
		},
		"password": {
			"type": "string",
			"minLength": 5,
			"maxLength": 20,
			"default": ""
		},
		"firstName": {
			"type": "string",
			"minLength": 1,
			"maxLength": 30,
			"default": ""
		},
		"lastName": {
			"type": "string",
			"minLength": 1,
			"maxLength": 30,
			"default": ""
		},
		"email": {
			"type": "string",
			"minLength": 6,
			"maxLength": 60,
			"format": "email",
			"default": ""
		},
		"completedTasks": {
			"type": "integer",
			"default": 0
		},
		"avatar": {
			"type": "string",
			"default": null
		}
	},
	"additionalProperties": false,
	"required": [ "username", "firstName", "lastName", "password", "email" ]
}
